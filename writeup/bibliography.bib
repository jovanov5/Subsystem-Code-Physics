@article{Bultinck.2017.Verstraete, 
year = {2017}, 
title = {{Fermionic matrix product states and one-dimensional topological phases}}, 
author = {Bultinck, Nick and Williamson, Dominic J. and Haegeman, Jutho and Verstraete, Frank}, 
