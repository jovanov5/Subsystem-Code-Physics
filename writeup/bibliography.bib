@article{Ashby-Pickering.2022.Wierzchucka, 
year = {2022}, 
title = {{Quantum state tomography, entanglement detection and Bell violation prospects in weak decays of massive particles}}, 
author = {Ashby-Pickering, Rachel and Barr, Alan J and Wierzchucka, Agnieszka}, 
journal = {arXiv}, 
doi = {10.48550/arxiv.2209.13990}, 
eprint = {2209.13990}, 
abstract = {{A rather general method for determining the spin density matrix of a multi-particle system from angular decay data is presented. The method is based on a Bloch parameterisation of the \$d\$-dimensional generalised Gell-Mann representation of \$\textbackslashrho\$ and exploits the associated Wigner- and Weyl-transforms on the sphere. Each parameter of a (possibly multipartite) spin density matrix can be measured from a simple average over an appropriate set of experimental angular decay distributions. The general procedures for both projective and non-projective decays are described, and the Wigner \$P\$ and \$Q\$ symbols calculated for the cases of spin-half, spin-one, and spin-3/2 systems. The methods are used to examine Monte Carlo simulations of \$pp\$ collisions for bipartite systems: \$pp\textbackslashrightarrow W\textasciicircum+W\textasciicircum-\$, \$pp\textbackslashrightarrow ZZ\$, \$pp\textbackslashrightarrow ZW\textasciicircum+\$, \$pp\textbackslashrightarrow W\textasciicircum+\textbackslashbar\{t\}\$, \$t\textbackslashbar\{t\}\$, and those from the Higgs boson decays \$H\textbackslashrightarrow WW\textasciicircum\{*\}\$ and \$H\textbackslashrightarrow ZZ\textasciicircum*\$. Measurements are proposed for entanglement detection, exchange symmetry detection and Bell inequality violation in bipartite systems.}}, 
local-url = {file://localhost/Users/jovanjovanovic/Library/Mobile%20Documents/com~apple~CloudDocs/Jovan's%20Files%20-%20Portable%20(iCloud)/Physics%20-%20RESEARCH/Research%20Interests%20Folder/Research%20Interests%2023Q4/2209.13990.pdf}
}
@article{Friedman.2019.Chalker, 
year = {2019}, 
title = {{Spectral Statistics and Many-Body Quantum Chaos with Conserved Charge}}, 
author = {Friedman, Aaron J. and Chan, Amos and Luca, Andrea De and Chalker, J. T.}, 
journal = {Physical Review Letters}, 
issn = {0031-9007}, 
doi = {10.1103/physrevlett.123.210603}, 
pmid = {31809174}, 
eprint = {1906.07736}, 
abstract = {{We investigate spectral statistics in spatially extended, chaotic many-body quantum systems with a conserved charge. We compute the spectral form factor K(t) analytically for a minimal Floquet circuit model that has a U(1) symmetry encoded via spin-1/2 degrees of freedom. Averaging over an ensemble of realizations, we relate K(t) to a partition function for the spins, given by a Trotterization of the spin-1/2 Heisenberg ferromagnet. Using Bethe ansatz techniques, we extract the “Thouless time” tTh demarcating the extent of random matrix behavior, and find scaling behavior governed by diffusion for K(t) at t≲tTh. We also report numerical results for K(t) in a generic Floquet spin model, which are consistent with these analytic predictions.}}, 
pages = {210603}, 
number = {21}, 
volume = {123}, 
local-url = {file://localhost/Users/jovanjovanovic/Library/Mobile%20Documents/com~apple~CloudDocs/Jovan's%20Files%20-%20Portable%20(iCloud)/Physics%20-%20RESEARCH/Research%20Interests%20Folder/Research%20Interests%2021Q2/1906.07736.pdf}
}
@article{Daniel.2020.Miyake, 
year = {2020}, 
title = {{Computational universality of symmetry-protected topologically ordered cluster phases on 2D Archimedean lattices}}, 
author = {Daniel, Austin K. and Alexander, Rafael N. and Miyake, Akimasa}, 
journal = {Quantum}, 
doi = {10.22331/q-2020-02-10-228}, 
eprint = {1907.13279}, 
abstract = {{What kinds of symmetry-protected topologically ordered (SPTO) ground states can be used for universal measurement-based quantum computation in a similar fashion to the 2D cluster state? 2D SPTO states are classified not only by global on-site symmetries but also by subsystem symmetries, which are fine-grained symmetries dependent on the lattice geometry. Recently, all states within so-called SPTO cluster phases on the square and hexagonal lattices have been shown to be universal, based on the presence of subsystem symmetries and associated structures of quantum cellular automata. Motivated by this observation, we analyze the computational capability of SPTO cluster phases on all vertex-translative 2D Archimedean lattices. There are four subsystem symmetries here called ribbon, cone, fractal, and 1-form symmetries, and the former three are fundamentally in one-to-one correspondence with three classes of Clifford quantum cellular automata. We conclude that nine out of the eleven Archimedean lattices support universal cluster phases protected by one of the former three symmetries, while the remaining lattices possess 1-form symmetries and have a different capability related to error correction.}}, 
pages = {228}, 
volume = {4}, 
local-url = {file://localhost/Users/jovanjovanovic/Library/Mobile%20Documents/com~apple~CloudDocs/Jovan's%20Files%20-%20Portable%20(iCloud)/Physics%20-%20RESEARCH/Research%20Interests%20Folder/Research%20Interests%2023Q4/1907.13279.pdf}
}
